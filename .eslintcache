} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/movie/NowPlaying\">\n          <MovieElement title=\"Now Playing\" subTitle=\"movies\" data_info={nowPlayingMovies} />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/movie/Upcoming\">\n          <MovieElement title=\"Upcoming\" subTitle=\"movies\" data_info={upcomingMovies} />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/movie/TopRated\">\n          <MovieElement title=\"Top Rated\" subTitle=\"movies\" data_info={topRatedMovies} />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/tv/TvPopular\">\n          <MovieElement title=\"Popular\" subTitle=\"TV Shows\" data_info={tvPopular} />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/tv/TvLatest\">\n          <MovieElement title=\"Latest\" subTitle=\"TV Shows\" data_info={onTv} />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/:id\">\n          <SreachMovies title=\"Latest\" movieSreach={movieSreach} subTitle=\"Sreach results of\" />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n        <Route exact path=\"/\">\n          <SreachMovies title=\"Latest\" movieSreach={movieSreach} subTitle=\"Sreach results of\" />\n          <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n        </Route>\n\n        <Route exact path=\"/tv/TvTopRated\">\n          <MovieElement title=\"Top Rated \" subTitle=\"TV Shows\" data_info={tvTopRated} />\n        </Route>\n        <Route path={`/movie/:id/`} component={MovieDetail} />\n        <Route path={`/actor/:id/`} component={ActorDetail} />\n        <Route\n          path={`/tv/gendre/:id/`}\n          render={(props) => (\n            <>\n              <GenresTv {...props} page={page} />\n              <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n            </>\n          )}\n        />\n\n        <Route\n          path={`/gendre/:id/`}\n          render={(props) => (\n            <>\n              <Genres {...props} page={page} />\n              <Pagination setPage={setPage} page={page} numbOfPages={numbOfPages} />\n            </>\n          )}\n        />\n        <Route path={`/tv/:id/`} component={MovieDetail} />\n        <Route path={`/gendre/:id/`} component={Genres} />\n      </Switch>\n    </div>\n  );\n};\n\nexport default MainContent;\n","/Users/durcak/Desktop/React/bef lazyload/src/components/sreachbar.js",[],"/Users/durcak/Desktop/React/bef lazyload/src/api/fetching.js",[],"/Users/durcak/Desktop/React/bef lazyload/src/components/GenresTv.js",["94"],"import React, { useEffect } from \"react\";\nimport MovieCard from \"./MovieCard\";\nimport { useFetchingUrl2 } from \"../api/fetching\";\nimport { MatchIdNames, gendresId } from \"../api/mapingForm\";\n\nconst GenresTv = ({ match, page }) => {\n  const genresSreachUrl = `https://api.themoviedb.org/3/discover/tv?api_key=a6cf54bca5a91f9a22017d7d14ad617a&language=en-US&sort_by=popularity.desc&include_adult=false&include_video=false&page=1&with_genres=${match.params.id}`;\n  const { object2 } = useFetchingUrl2(genresSreachUrl, page);\n  const result = MatchIdNames(gendresId, match.params.id);\n\n  return (\n    <div className=\" mb-10 mt-5 flex justify-self-center content-center self-center place-content-center items-center justify-center    justify-items-center\t\">\n      <div className=\"mt-3 text-center   \">\n        <div className=\"font-rubik font-light italic -mb-1 mt-8 sm:m-0 self-center\t text-2xl \">{result.name}</div>\n        <div className=\"font-mukta -mb-10  sm:mb-0 font-bold\">i